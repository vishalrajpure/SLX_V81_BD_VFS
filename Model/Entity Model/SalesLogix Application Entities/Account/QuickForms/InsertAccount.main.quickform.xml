{
	DialogService.ShowMessage("Enter the AccountName...");
	return;
}
if(account.MainPhone == null)
{
	DialogService.ShowMessage("Enter the MainPhone...");
	return;
}
string qry = "Select Account From Account where Account = '" + AccountName.Text.Trim() + "'";
